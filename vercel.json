{
  "version": 2,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/hooks-playground/(.*)",
      "dest": "/hooks-playground/$1"
    },
    {
      "src": "/state-management/(.*)",
      "dest": "/state-management/$1"
    },
    {
      "src": "/routing-system/(.*)",
      "dest": "/routing-system/$1"
    },
    {
      "src": "/styling-solutions/(.*)",
      "dest": "/styling-solutions/$1"
    },
    {
      "src": "/performance/(.*)",
      "dest": "/performance/$1"
    },
    {
      "src": "/ssr-solutions/(.*)",
      "dest": "/ssr-solutions/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
